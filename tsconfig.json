{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "dist",
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "es2015.collection",
      "es2015.core",
      "es2015.generator",
      "es2015.iterable",
      "es2015.promise",
      "es2015.proxy",
      "es2015.reflect",
      "es2015.symbol",
      "es2015.symbol.wellknown",
      "es2016",
      "es2016.array.include",
      "es2017",
      "es2017.object",
      "es2017.sharedmemory",
      "es2017.typedarrays",
      "es2018",
      "es2018.promise",
      "es2018.regexp",
      "esnext",
      "esnext.array",
      "esnext.asynciterable"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "strict": true,
    "target": "es5",
  },
  "exclude": [
    "dist",
    "node_modules",
  ]
}